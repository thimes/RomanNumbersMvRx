<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/entryFragment"
    >

    <fragment
        android:id="@+id/entryFragment"
        android:name="com.himes.romannumerals.entryscreen.EntryScreenFragment"
        android:label="Entry Screen"
        >
        <action
            android:id="@+id/action_entryFragment_to_resultFragment"
            app:destination="@id/resultFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.himes.romannumerals.resultscreen.ResultFragment"
        android:label="ResultFragment"
        />

</navigation>